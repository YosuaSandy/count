<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sandy.quickcount"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:name="com.example.sandy.quickcount.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/FullScreen" >
        <activity android:name="com.example.sandy.quickcount.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.sandy.quickcount.Mainmenu"
            android:icon="@drawable/woclass"
            android:label=".Mainmenu"
            android:theme="@style/AppTheme2" />
        <activity
            android:name="com.example.sandy.quickcount.Profil"
            android:icon="@drawable/woclass"
            android:label="Profil"
            android:theme="@style/AppTheme2" />
        <activity
            android:name="com.example.sandy.quickcount.Keterangan"
            android:icon="@drawable/woclass"
            android:label="@string/title_activity_keterangan"
            android:theme="@style/AppTheme2" />
        <activity
            android:name="com.example.sandy.quickcount.Pemilihan"
            android:icon="@drawable/woclass"
            android:label="@string/title_activity_pemilihan"
            android:theme="@style/AppTheme2" />

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>